{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Postgres": {
    "ConnectionString": "Server=localhost;Port=5533;Database=orders-service;User ID=postgres;Password=postgres"
  },
  "Kafka": {
    "Consumers": {
      "PreOrder": {
        "Topic": "pre_orders",
        "Config": {
          "GroupId": "order_service_pre_orders",
          "BootstrapServers": "localhost:29091",
          "EnableAutoCommit": false
        }
      },
      "OrdersEvent": {
        "Topic": "orders_events",
        "Config": {
          "GroupId": "order_service_orders_events",
          "BootstrapServers": "localhost:29091",
          "EnableAutoCommit": false
        }
      }
    },
    "Producers": {
      "NewOrder": {
        "Topic": "new_orders",
        "Config": {
          "BootstrapServers": "localhost:29091"
        }
      }
    }
  },
  "Redis": {
    "ConnectionString": "localhost:8009",
    "TTL": 5
  }
}
